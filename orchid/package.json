{
    "name": "orch",
    "version": "0.0.1",
    "engines": {
        "vscode": ">=0.9.0-pre.1"
    },
    "publisher": "MarcoFerretti",
    "icon":"images/orchidlogo.png",
    "repository": "https://github.com/marcoamsterdam/orchid-vscode",
    "contributes": {
        "languages": [{
            "id": "orchid",
            "aliases": ["orchid"],
            "extensions": [".orchid",".orch"],
            "configuration": "./language-configuration.json",
            "icon": {
                "light": "images/orchidicon.png",
                "dark": "images/orchidicon.png"
              }
        }],
        "grammars": [{
            "language": "orchid",
            "scopeName": "source.orchid",
            "path": "./syntaxes/orchid.tmLanguage"
        }],
        "comments": {
            "lineComment": "#"
        },
        "brackets": [
            ["{", "}"],
            ["[", "]"],
            ["(", ")"]
        ]
    }
}