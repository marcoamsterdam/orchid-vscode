{
    "name": "orchid",
    "version": "0.0.1",
    "engines": {
        "vscode": ">=0.9.0-pre.1"
    },
    "publisher": "Marco Ferretti",
    "icon":"./images/orchidlogo.png",
    "contributes": {
        "languages": [{
            "id": "orchid",
            "aliases": ["orchid"],
            "extensions": [".orchid",".orch"],
            "configuration": "./language-configuration.json",
            "icon": {
                "light": "./images/orchidicon.png",
                "dark": "./images/orchidicon.png"
              }
        }],
        "grammars": [{
            "language": "orchid",
            "scopeName": "source.orchid",
            "path": "./syntaxes/orchid.tmLanguage"
        }],
        "comments": {
            "lineComment": "#"
        },
        "brackets": [
            ["{", "}"],
            ["[", "]"],
            ["(", ")"]
        ]
    }
}